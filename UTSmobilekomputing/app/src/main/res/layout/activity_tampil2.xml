<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tampil2"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center_horizontal"
        android:text="DATA UJIAN"
        android:textAlignment="center"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="5dp" />

    <TextView
        android:id="@+id/nim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nim"
        android:textSize="13dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="57dp" />

    <TextView
        android:id="@+id/nama_mahasiswa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nama Mahasiswa"
        android:textSize="13dp"
        android:layout_marginLeft="5dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="86dp" />


        <EditText
            android:id="@+id/matakuliah"
            android:layout_width="match_parent"
            android:layout_height="53dp"
            android:ems="10"
            android:hint="Nama Matakuliah"
            android:inputType="textPersonName"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="120dp" />


        <EditText
            android:id="@+id/sks"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="SKS"
            android:inputType="number"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="120dp"/>



    <TextView
        android:id="@+id/kelas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        android:text="Kelas"
        android:textSize="13dp"
        android:layout_marginLeft="5dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="239dp" />



        <EditText
            android:id="@+id/txt_tgl"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:clickable="true"
            android:editable="false"
            android:focusable="false"
            android:hint="Tanggal" />




        <EditText
            android:id="@+id/sifat"
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:hint="Sifat Ujian"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="120dp" />


        <EditText
            android:id="@+id/studi"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:hint="Program Studi"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="120dp" />


        <EditText
            android:id="@+id/dosen"
            android:layout_width="match_parent"
            android:layout_height="54dp"
            android:hint="Dosen Pengampu"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="120dp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <Button
            android:id="@+id/kirim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginRight="30dp"
            android:hint="SUBMIT"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="70dp"
            tools:layout_editor_absoluteY="497dp" />

        <Button
            android:id="@+id/reset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:hint="RESET"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="256dp"
            tools:layout_editor_absoluteY="497dp" />

    </LinearLayout>
</LinearLayout>